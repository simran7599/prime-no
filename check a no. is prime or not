#include<conio.h>
#include<stdio.h>
int lcm(int,int );
void main()
{int a,b,result;
printf("enter 2 no.");
scanf("%d %d",&a,&b);
result=lcm(a,b);
printf("LCM of %d and %d is %d'a,b,result")
return 0;
}
int lcm(int a,int b)
{static int common=1;
if(commom %a ==0 && common %b==0)
{return common;
}
common ++;
lcm(a,b);
return common;
}
